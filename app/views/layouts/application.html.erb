  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="row">
    <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
      <h1>Bog Application</h1>
    </div>
    <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
    </div>
  </div>
    <div class="row">
      <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8">
    <%= yield %>

      </div>
      <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
  <ul class="nav bog-nav nav-stacked">
    <li role="presentation"><%= link_to "Home", root_path %></li>
    <li role="presentation"><%= link_to "Creatures", creatures_path %></li>
    <li><%= link_to "Tags", tags_path %></li>
  </ul>
      </div>
    </div>



  </div>
</body>
</html>
